########################################
# Server
########################################
PORT=8081
########################################
# Frontend Base URL
########################################
FRONTEND_BASE_URL=https://livekit-agent-builder.vercel.app


########################################
# LiveKit
########################################
LIVEKIT_URL=wss://your-livekit-domain
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

########################################
# OpenAI
########################################
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4o-mini

########################################
# Deepgram
########################################
DEEPGRAM_API_KEY=your-deepgram-api-key
DEEPGRAM_MODEL=nova-2-phonecall
DEEPGRAM_ENDPOINTING=25

########################################
# Cartesia (TTS)
########################################
CARTESIA_API_KEY=your-cartesia-api-key
CARTESIA_VOICE_ID=your-cartesia-voice-id

########################################
# ElevenLabs (TTS)
########################################
ELEVEN_API_KEY=

########################################
# TTS selection
########################################
TTS_PROVIDER=cartesia

########################################
# Database
########################################
DATABASE_URL=postgresql://user:password@host/dbname?sslmode=require

########################################
# Langfuse (optional)
########################################
LANGFUSE_PUBLIC_KEY=
LANGFUSE_SECRET_KEY=
LANGFUSE_BASE_URL=https://cloud.langfuse.com

########################################
# Agent behavior (optional)
########################################
AGENT_ALLOW_INTERRUPTIONS=true
AGENT_INTERRUPT_SPEECH_DURATION=500
AGENT_INTERRUPT_MIN_WORDS=0
AGENT_PREEMPTIVE_SYNTHESIS=false
AGENT_MIN_ENDPOINTING_DELAY=650

########################################
# VAD (optional)
########################################
VAD_MIN_SPEECH_DURATION=0.1
VAD_MIN_SILENCE_DURATION=0.5
VAD_PREFIX_PADDING_DURATION=0.5
VAD_MAX_BUFFERED_SPEECH=60.0
VAD_ACTIVATION_THRESHOLD=0.5
VAD_SAMPLE_RATE=16000
VAD_FORCE_CPU=false


